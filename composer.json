{
    "name": "scriptotek/alma-newbooks-db",
    "description": "NewBooksDb.",
    "keywords": ["alma"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "guzzlehttp/guzzle": "^6.2",
        "league/csv": "^8.1",
        "voku/portable-utf8": "^3.0",
        "laravelcollective/html": "~5.3",
        "laravelista/ekko": "^1.2",
        "mayconbordin/rss-l5": "~1.1",
        "barryvdh/laravel-debugbar": "^2.2",
        "scriptotek/alma-client": "^0.5.1",
        "aacotroneo/laravel-saml2": "^0.8.0",
        "arcanedev/log-viewer": "~4.0",
        "lstrojny/functional-php": "^1.4"

    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
