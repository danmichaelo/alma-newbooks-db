{
    "name": "scriptotek/alma-newbooks-db",
    "description": "NewBooksDb.",
    "keywords": ["alma"],
    "license": "Apache-2.0",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "aacotroneo/laravel-saml2": "^2.0",
        "arcanedev/log-viewer": "~4.0",
        "barryvdh/laravel-cors": "^0.11.3",
        "barryvdh/laravel-debugbar": "^3.1",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "^6.2",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "~5.3",
        "laravelista/ekko": "^1.2",
        "league/csv": "^8.1",
        "lstrojny/functional-php": "^1.4",
        "mayconbordin/rss-l5": "~1.1",
        "predis/predis": "^1.1",
        "scriptotek/alma-client": "^0.5.1",
        "twig/twig": "^1.30",
        "voku/portable-utf8": "^3.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
